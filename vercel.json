{
    "version": 2,
    "builds": [
      { "src": "/backend/**", "use": "@vercel/node" },
      { "src": "/frontend/**", "use": "@vercel/static" }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/server.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }
  
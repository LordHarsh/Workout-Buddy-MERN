{
  "version": 2,
  "builds": [
    {
      "src": "/backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "/frontend/build",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/([^/]+)",
      "dest": "/backend/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ]
}
